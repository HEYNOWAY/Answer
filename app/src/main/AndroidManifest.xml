<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.luos.demofortest">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.luos.answer.ui.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="com.example.luos.answer.ui.activity.AnswerContentActivity"/>
        <activity android:name="com.example.luos.answer.ui.activity.BaseActivity">
        </activity>
        <activity android:name="com.example.luos.answer.ui.activity.AnswerListActivity">
        </activity>
        <activity
            android:name="com.example.luos.answer.ui.activity.UserInfoActivity"
            android:label="@string/title_activity_user_info"
            android:theme="@style/AppTheme">
        </activity>
        <activity android:name="com.example.luos.answer.ui.activity.SearchActivity">
        </activity>
    </application>

</manifest>